{
  "common": {
    "name": "solarprognose",
    "version": "0.0.1",
    "title": "SolarPrognose",
    "desc": {
      "en": "Fetches solar forecasts from solarprognose.de",
      "de": "Gebühren Solarprognose von solarprognose.de",
      "ru": "Солнечные прогнозы от Solarprognose.de",
      "pt": "Fetches previsões solares de solarprognose.de",
      "nl": "Fetches zonnevoorspellingen van zonneprognose.de",
      "fr": "Fetches prévisions solaires de solarprognose.de",
      "it": "Fetches previsioni solari da solarprognose.de",
      "es": "Pronósticos solares de Solarprognose.de",
      "pl": "Fetches prognozy słoneczne z solarprognose.de",
      "uk": "Fetches сонячні прогнози від solarprognose.de",
      "zh-cn": "从太阳预测中获取太阳预报.de"
    },
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "icon": "solarprognose.png",
    "enabled": true,
    "messagebox": true,
    "materialize": true,
    "keywords": [
      "solar",
      "forecast",
      "energy",
      "photovoltaic"
    ],
    "type": "weather",
    "languages": [
      "en",
      "de",
      "ru",
      "pt",
      "nl",
      "fr",
      "it",
      "es",
      "pl",
      "uk",
      "zh-cn"
    ],
    "restartAdapters": [],
    "adminUI": {
      "config": "json"
    },
    "dataSource": "poll",
    "dependencies": [
      {
        "js-controller": ">=3.0.0"
      }
    ],
    "protectedNative": ["apiKey"],
    "encryptedNative": ["apiKey"]
  },
  "native": {
    "accessToken": "",
    "inverterId": 0,
    "item": "inverter",
    "project": "ioBroker Integration",
    "startDay": -2,
    "endDay": 3,
    "defaultApiTime": 2133,
    "updateEveryXHour": 2,
    "apiType": "hourly",
    "maxRetries": 3,
    "retryDelay": 5000,
    "cacheTTL": 300000,
    "requestTimeout": 15000
  },

  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information",
        "desc": "Adapter information"
      }
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "name": "Connection status",
        "type": "boolean",
        "role": "indicator.connected",
        "read": true,
        "write": false,
        "def": false
      }
    }
  ]
}
